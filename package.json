{
  "name": "tfjs-utils",
  "version": "0.9.0",
  "description": "Collection of generic JS utils when working with TF models",
  "main": "signature.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node --trace-warnings --trace-uncaught --unhandled-rejections=warn --stack-trace-limit=50 --no-deprecation ./detector.js",
    "kernels": "esbuild --bundle --format=esm --minify --platform=browser --target=esnext --outfile=dist/kernels.esm.js src/kernels.ts"
  },
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^3.12.0",
    "@vladmandic/build": "^0.6.6",
    "@vladmandic/pilogger": "^0.3.5",
    "@vladmandic/tfjs": "github:vladmandic/tfjs",
    "canvas": "^2.8.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "chokidar": "^3.5.2",
    "esbuild": "^0.14.3",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0"
  }
}
