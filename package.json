{
  "name": "tfjs-utils",
  "version": "0.10.0",
  "description": "Collection of generic JS utils when working with TF models",
  "main": "signature.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node --trace-warnings --trace-uncaught --unhandled-rejections=warn --stack-trace-limit=50 --no-deprecation ./detector.js",
    "kernels": "esbuild --bundle --format=esm --minify --platform=browser --target=esnext --outfile=dist/kernels.esm.js src/kernels.ts",
    "serve": "build --profile serve"
  },
  "author": "Vladimir Mandic <mandic00@live.com>",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^3.20.0",
    "@tensorflow/tfjs-node-gpu": "^3.20.0",
    "@vladmandic/build": "^0.7.12",
    "@vladmandic/pilogger": "^0.4.6",
    "@vladmandic/tfjs": "github:vladmandic/tfjs",
    "canvas": "^2.10.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "chokidar": "^3.5.3",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0"
  }
}
